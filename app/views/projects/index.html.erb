<div class="container">
  <h1>Projects</h1>
  <p>This page contains nu most recent projects</p>

  <div id="projects">
    <% @projects.each do |project| %>
      <% if current_user&.admin? %>
        <%= render 'projects/draggable', project: project %>
      <% else %>
        <%= render project %>
      <% end %>
    <% end %>
  </div>
</div>